# Environment variables for Admin Dashboard
# Copy to .env and customize
# Note: db_env.sh is the single source of truth for ports

# Database (PostgreSQL) - sourced from db_env.sh
DATABASE_URL_ASYNC=postgresql+asyncpg://trading:trading123@localhost:5433/exchange_info_db

# Security
ADMIN_SECRET_KEY=change-me-in-production-please-use-32-chars-minimum

# Server - ports managed by db_env.sh
# ADMIN_HOST=0.0.0.0
# ADMIN_PORT=8002  # Dev default, CI uses 8001

# Default admin credentials
DEFAULT_ADMIN_USERNAME=admin
DEFAULT_ADMIN_PASSWORD=admin123
